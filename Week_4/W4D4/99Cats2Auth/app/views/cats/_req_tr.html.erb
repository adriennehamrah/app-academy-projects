<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? %>
      <form action="<%= approve_cat_rental_request_url(req) %>" method="POST">
          <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
        <input type= <%= current_user == cat.owner ? "submit" : "hidden" %> value="Approve" />
      </form>
      <form action="<%= deny_cat_rental_request_url(req) %>" method="POST">
          <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>">
        <input type=<%= current_user == cat.owner ? "submit" : "hidden" %> value="Deny" />
      </form>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
  <td><%= cat.owner.user_name %></td>
  <td><%= req.requester.user_name %></td>
</tr>
